{
  "name": "grunt-cssnext",
  "version": "0.0.0",
  "description": "Use tomorrow's CSS syntax, today, using cssnext through Grunt",
  "keywords": [
    "css",
    "preprocessor",
    "postprocessor",
    "rework",
    "postcss",
    "autoprefixer",
    "grunt",
    "gruntplugin"
  ],
  "author": "MoOx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/putaindecode/grunt-cssnext.git"
  },
  "homepage": "https://cssnext.putaindecode.io",
  "bugs": {
    "url": "https://github.com/putaindecode/grunt-cssnext/issues"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "index.js"
  ],
  "dependencies": {
    "cssnext": "^0.2.0",
    "each-async": "^0.1.1"
  },
  "devDependencies": {
    "grunt": "git://github.com/gruntjs/grunt.git",
    "jscs": "^1.5.9",
    "jshint": "^2.5.2",
    "jshint-stylish": "^0.4.0",
    "tap-colorize": "^1.2.0",
    "tape": "^2.13.4"
  },
  "scripts": {
    "jscs": "jscs *.js **/*.js",
    "jshint": "jshint . --exclude-path .gitignore --reporter node_modules/jshint-stylish/stylish.js",
    "lint": "npm run jscs && npm run jshint",
    "tape": "tape test | tap-colorize",
    "test": "npm run lint && npm run tape"
  }
}
